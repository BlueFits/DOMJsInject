{
  "name": "dom-js-inject",
  "displayName": "Dom JS Inject",
  "publisher": "ChristianRojas",
  "description": "Inject code to webpages to see the changes made in real time",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "github.com/BlueFits/DOMJsInject"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dom-js-inject.launch",
    "onCommand:dom-js-inject.gen_pers_template"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dom-js-inject.launch",
        "title": "Launch JS Inject"
      },
      {
        "command": "dom-js-inject.gen_pers_template",
        "title": "Generate PERS Template"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/puppeteer": "^5.4.6",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/validator": "^13.7.2",
    "cheerio": "^1.0.0-rc.10",
    "puppeteer": "^13.7.0",
    "validator": "^13.7.0"
  }
}
